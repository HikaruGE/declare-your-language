module to-xml
imports src-gen/signatures/Programs-sig
imports classes-to-xml
imports expressions-to-xml
rules

  to-xml : Program(name, cs, e) -> $[
  	<program>
  	  <name>[name]</name>
  	  <classes>
  	    [<map(to-xml)> cs]
  	  </classes>
  	  <run>
  	    [<to-xml>e]
  	  </run>
  	</program>
  ]